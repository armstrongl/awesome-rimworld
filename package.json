{
  "name": "awesome-rimworld",
  "version": "1.0.0",
  "description": "A curated list of awesome RimWorld resources",
  "private": true,
  "scripts": {
    "lint": "markdownlint-cli2 '**/*.md' '#node_modules'",
    "lint:fix": "markdownlint-cli2 --fix '**/*.md' '#node_modules'",
    "format": "prettier --check '**/*.md'",
    "format:fix": "prettier --write '**/*.md'",
    "awesome-lint": "awesome-lint",
    "toc": "doctoc README.md --github --title '## Contents'",
    "test": "npm run lint && npm run format && npm run awesome-lint"
  },
  "devDependencies": {
    "awesome-lint": "^2.2.2",
    "doctoc": "^2.2.1",
    "markdownlint-cli2": "^0.20.0",
    "prettier": "^3.4.2"
  },
  "keywords": [
    "awesome",
    "awesome-list",
    "curated-list",
    "rimworld"
  ],
  "license": "CC0-1.0"
}
